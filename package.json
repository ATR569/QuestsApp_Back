{
  "name": "questsapp_back",
  "version": "1.0.0",
  "description": "API do QuestsApp",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node ./index.ts\" -e ts",
    "start": "node ./dist/index.js",
    "test": "nyc --clean --all --reporter=text-summary mocha test/**/*.spec.ts || exit 0",
    "test:unit": "nyc --clean --all --reporter=text-summary mocha test/unit/**/*.spec.ts || exit 0",
    "test:integration": "nyc --clean --all --reporter=text-summary mocha test/integration/**/*.spec.ts"
  },
  "keywords": [
    "QuestsApp",
    "Backend",
    "Software Educacional"
  ],
  "author": "Adson MacÃªdo, Thairam Michel, Ramon Rodrigues, Jardesson Elliudo",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.17",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/http-status-codes": "^1.2.0",
    "@types/mocha": "^8.2.2",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.10.4",
    "@types/node": "^14.14.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "chai": "^4.3.4",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "module-alias": "^2.2.2",
    "mongoose": "^5.12.3"
  },
  "nyc": {
    "include": [
      "**/*.tsx",
      "**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/*",
      "config/*"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  }
}
